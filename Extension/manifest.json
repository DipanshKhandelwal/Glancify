{
  "manifest_version": 2,

  "name": "Glancify",
  "description": "This extensions collects your News Feeds and Notfications and displays it on the New Tab",
  "version": "1.0",

  "browser_action": {
    "default_popup": "extension_popup.html"
  },

  "icons": {
    "16": "Images/glancify_icon.png",
    "32": "Images/glancify_icon.png",
    "64": "Images/glancify_icon.png",
    "128": "Images/glancify_icon.png"
  },

  "background": {
    "page": "extension_newtab.html"
  },

  "chrome_url_overrides": {
    "newtab": "extension_newtab.html"
  },

  "permissions": [
    "notifications",
    "https://twitter.com/i/notifications"
  ]

}
