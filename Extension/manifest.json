{
  "manifest_version": 2,

  "name": "Glancify",
  "description": "This extensions collects your News Feeds and Notfications and displays it on the New Tab",
  "version": "1.0",

  "browser_action": {
    "default_icon": "default.png",
    "default_popup": "main_popup.html"
  },

  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },

  "chrome_url_overrides": {
    "newtab": "custom_newtab.html"
  }
}
